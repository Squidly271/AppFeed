  <Overview>Energy monitor</Overview>
  <Description>Energy monitor</Description>
  <Registry>https://registry.hub.docker.com/r/snoopy86/emoncms-docker</Registry>
  <Repository>snoopy86/emoncms-docker</Repository>
  <DonateText>Buy me a beer.</DonateText>
  <DonateImg>https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif</DonateImg>
  <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;hosted_button_id=FDMP56NCG5A26&amp;source=url</DonateLink>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>MYSQL_HOST</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MYSQL_PORT</Name>
      <Value>3306</Value>
    </Variable>
    <Variable>
      <Name>MYSQL_USER</Name>
      <Value>emoncms</Value>
    </Variable>
    <Variable>
      <Name>MYSQL_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MYSQL_DATABASE</Name>
      <Value>emoncms</Value>
    </Variable>
    <Variable>
      <Name>MYSQL_RANDOM_ROOT_PASSWORD</Name>
      <Value>yes</Value>
    </Variable>
    <Variable>
      <Name>REDIS_ENABLED</Name>
      <Value>false</Value>
    </Variable>
    <Variable>
      <Name>REDIS_HOST</Name>
      <Value>127.0.0.1</Value>
    </Variable>
    <Variable>
      <Name>REDIS_PORT</Name>
      <Value>6379</Value>
    </Variable>
    <Variable>
      <Name>REDIS_PREFIX</Name>
      <Value>'emoncms'</Value>
    </Variable>
    <Variable>
      <Name>MQTT_ENABLED</Name>
      <Value>false</Value>
    </Variable>
    <Variable>
      <Name>MQTT_HOST</Name>
      <Value>127.0.0.1</Value>
    </Variable>
    <Variable>
      <Name>MQTT_USER</Name>
      <Value>mqtt</Value>
    </Variable>
    <Variable>
      <Name>MQTT_PASSWORD</Name>
      <Value>mqtt</Value>
    </Variable>
    <Variable>
      <Name>MQTT_BASETOPIC</Name>
      <Value>emon</Value>
    </Variable>
    <Variable>
      <Name>PHPFINA_DIR</Name>
      <Value>/var/opt/emoncms/phpfina/</Value>
    </Variable>
    <Variable>
      <Name>PHPTIMESERIES_DIR</Name>
      <Value>/var/opt/emoncms/phptimeseries/</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8998</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>1883</HostPort>
        <ContainerPort>1883</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/emoncms/phpfina</HostDir>
      <ContainerDir>/var/opt/emoncms/phpfina/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/emoncms/phptimeseries/phpfina</HostDir>
      <ContainerDir>/var/opt/emoncms/phptimeseries/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Icon>https://emoncms.org/Modules/site/emoncms_front.png</Icon>
  <TemplateURL>https://raw.githubusercontent.com/snoopy86/docker-repo/master/snoopy/emoncms.xml</TemplateURL>
  <Category>Productivity:</Category>
  <templatePath>/tmp/GitHub/AppFeed/templates/snoopy86sRepository/snoopy/emoncms.xml</templatePath>
