  <TemplateURL>https://raw.githubusercontent.com/Josh5/unraid-docker-templates/master/josh5/teamcity-agent.xml</TemplateURL>
  <Icon>https://seeklogo.com/images/T/team-city-logo-8FEEB62EC1-seeklogo.com.png</Icon>
  <PostArgs></PostArgs>
  <CPUset></CPUset>
