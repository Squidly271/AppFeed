<?xml version="1.0" encoding="UTF-8"?>
<Container>
  <TemplateURL>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/nzbhydra.xml</TemplateURL>
  <Name>binhex-nzbhydra</Name>
  <Overview>
    NZBHydra is a meta search for NZB indexers. It provides easy access to a number of raw and newznab based indexers. You can search all your indexers from one place and use it as indexer source for tools like Sonarr or CouchPotato.
  </Overview>
  <Support>https://forums.lime-technology.com/topic/62378-support-binhex-nzbhydra/</Support>
  <Registry>https://registry.hub.docker.com/r/binhex/arch-nzbhydra/</Registry>
  <GitHub>https://github.com/binhex/arch-nzbhydra</GitHub>
  <Repository>binhex/arch-nzbhydra</Repository>
  <Project>https://github.com/theotherp/nzbhydra</Project>
  <Deprecated>true</Deprecated>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5075</HostPort>
        <ContainerPort>5075</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/data</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5075]/</WebUI>
  <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/nzbhydra-icon.png</Icon>
  <DonateText>If you appreciate my work, then please consider buying me a beer :D</DonateText>
  <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;hosted_button_id=MM5E27UX6AUU4</DonateLink>
  <DonateImg>https://www.paypal.com/en_US/i/btn/btn_donate_SM.gif</DonateImg>
  <Category>Downloaders: MediaApp:Video</Category>
  <Description>NZBHydra is a meta search for NZB indexers. It provides easy access to a number of raw and newznab based indexers. You can search all your indexers from one place and use it as indexer source for tools like Sonarr or CouchPotato.</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/BinhexsRepository/binhex/nzbhydra.xml</templatePath>
</Container>
