<?xml version="1.0" encoding="UTF-8"?>
<Container version="2">
  <TemplateURL>https://raw.githubusercontent.com/diamkil/docker-templates/master/diamkil/notea.xml</TemplateURL>
  <Name>Notea</Name>
  <Overview>
    Self hosted note taking, alternative to Notions. Look at Github for config
  </Overview>
  <Support>https://forums.unraid.net/topic/107577-support-diamkil-notea/</Support>
  <Registry>https://registry.hub.docker.com/r/diamkil/notea/</Registry>
  <Repository>diamkil/notea</Repository>
  <Project>https://github.com/diamkil/docker-notea</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/Notea</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <Icon>https://raw.githubusercontent.com/diamkil/docker-templates/master/diamkil/images/notea.png</Icon>
  <Config Name="Notea Port" Target="3000" Default="" Mode="tcp" Description="Port for Notea WebUI" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="MinIO Port" Target="9000" Default="" Mode="tcp" Description="Port for MinIO WebUI" Type="Port" Display="always" Required="false" Mask="false"></Config>
  <Config Name="WebUI Password" Target="PASSWORD" Default="" Mode="" Description="Set the password to use on Notea's Website" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="MinIO User" Target="STORE_USER" Default="" Mode="" Description="User for MinIO Backend" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="MinIO Password" Target="STORE_PASSWORD" Default="" Mode="" Description="Password for MinIO Backend" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Description>Self hosted note taking, alternative to Notions. Look at Github for config</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/diamkilsRepository/diamkil/notea.xml</templatePath>
</Container>
