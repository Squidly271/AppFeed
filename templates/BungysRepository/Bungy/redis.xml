<?xml version="1.0" encoding="UTF-8"?>
<Container>
  <Name>redis</Name>
  <Overview>
    Redis for use with gitlab.
	Install this docker before installing the gitlab docker.
  </Overview>
  <Support>http://lime-technology.com/forum/index.php?topic=38930.0</Support>
  <Registry>https://registry.hub.docker.com/r/sameersbn/redis/</Registry>
  <GitHub>https://github.com/sameersbn/docker-redis</GitHub>
  <Repository>sameersbn/redis</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish></Publish>
  </Networking>
  <Environment></Environment>
  <Data></Data>
  <Icon>https://cdn4.iconfinder.com/data/icons/redis-2/1451/Untitled-2-128.png</Icon>
  <TemplateURL>https://raw.githubusercontent.com/jshridha/templates/master/Bungy/redis.xml</TemplateURL>
  <Category>Tools:Utilities</Category>
  <Description>Redis for use with gitlab.
	Install this docker before installing the gitlab docker.</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/BungysRepository/Bungy/redis.xml</templatePath>
</Container>
