<?xml version="1.0" encoding="UTF-8"?>
<Container version="2">
  <Name>cloudflare-ddns-ghcr</Name>
  <Repository>ghcr.io/hotio/cloudflare-ddns</Repository>
  <Registry>https://github.com/users/hotio/packages/container/package/cloudflare-ddns</Registry>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://hotio.dev/discord</Support>
  <Project>https://hotio.dev/containers/cloudflare-ddns</Project>
  <Icon>https://raw.githubusercontent.com/docker-hotio/unraid-templates/master/hotio/img/cloudflare-ddns.png</Icon>
  <Overview>Cloudflare DDNS updates for both ipv4 and ipv6.</Overview>
  <Description>Cloudflare DDNS updates for both ipv4 and ipv6.</Description>
  <WebUI></WebUI>
  <PostArgs></PostArgs>
  <Network>bridge</Network>
  <Config Name="Host Path for /config" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="always-hide" Required="true" Mask="false"></Config>
  <Config Name="INTERVAL" Target="INTERVAL" Default="300" Mode="" Description="Container Variable: INTERVAL" Type="Variable" Display="always-hide" Required="false" Mask="false"></Config>
  <Config Name="DETECTION_MODE" Target="DETECTION_MODE" Default="dig-whoami.cloudflare|dig-google.com|dig-opendns.com|curl-icanhazip.com|curl-wtfismyip.com|curl-showmyip.ca|curl-da.gd|curl-seeip.org|curl-ifconfig.co" Mode="" Description="Container Variable: DETECTION_MODE" Type="Variable" Display="always-hide" Required="false" Mask="false"></Config>
  <Config Name="LOG_LEVEL" Target="LOG_LEVEL" Default="3|2|1|0" Mode="" Description="Container Variable: LOG_LEVEL&amp;lt;br&amp;gt;3 - Debug logging&amp;lt;br&amp;gt;2 - Verbose logging&amp;lt;br&amp;gt;1 - Normal logging&amp;lt;br&amp;gt;0 - No logging" Type="Variable" Display="always-hide" Required="false" Mask="false"></Config>
  <Config Name="APPRISE" Target="APPRISE" Default="" Mode="" Description="Container Variable: APPRISE&amp;lt;br&amp;gt;Send notifications with Apprise." Type="Variable" Display="always-hide" Required="false" Mask="false"></Config>
  <Config Name="CF_USER" Target="CF_USER" Default="" Mode="" Description="Container Variable: CF_USER&amp;lt;br&amp;gt;Leave blank if you use CF_APITOKEN." Type="Variable" Display="always-hide" Required="false" Mask="false"></Config>
  <Config Name="CF_APIKEY" Target="CF_APIKEY" Default="" Mode="" Description="Container Variable: CF_APIKEY&amp;lt;br&amp;gt;Leave blank if you use CF_APITOKEN." Type="Variable" Display="always-hide" Required="false" Mask="true"></Config>
  <Config Name="CF_APITOKEN" Target="CF_APITOKEN" Default="" Mode="" Description="Container Variable: CF_APITOKEN" Type="Variable" Display="always-hide" Required="false" Mask="true"></Config>
  <Config Name="CF_APITOKEN_ZONE" Target="CF_APITOKEN_ZONE" Default="" Mode="" Description="Container Variable: CF_APITOKEN_ZONE" Type="Variable" Display="always-hide" Required="false" Mask="true"></Config>
  <Config Name="CF_HOSTS" Target="CF_HOSTS" Default="" Mode="" Description="Container Variable: CF_HOSTS&amp;lt;br&amp;gt;Example: test.example.com;test.foobar.com;test2.foobar.com" Type="Variable" Display="always-hide" Required="true" Mask="false"></Config>
  <Config Name="CF_ZONES" Target="CF_ZONES" Default="" Mode="" Description="Container Variable: CF_ZONES&amp;lt;br&amp;gt;Example: example.com;foobar.com;foobar.com" Type="Variable" Display="always-hide" Required="true" Mask="false"></Config>
  <Config Name="CF_RECORDTYPES" Target="CF_RECORDTYPES" Default="" Mode="" Description="Container Variable: CF_RECORDTYPES&amp;lt;br&amp;gt;Example: A;A;AAAA" Type="Variable" Display="always-hide" Required="true" Mask="false"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="advanced-hide" Required="false" Mask="false"></Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="advanced-hide" Required="false" Mask="false"></Config>
  <Config Name="UMASK" Target="UMASK" Default="002" Mode="" Description="Container Variable: UMASK" Type="Variable" Display="advanced-hide" Required="false" Mask="false"></Config>
  <Config Name="ARGS" Target="ARGS" Default="" Mode="" Description="Container Variable: ARGS" Type="Variable" Display="advanced-hide" Required="false" Mask="false"></Config>
  <Category>Network:DNS</Category>
  <templatePath>/tmp/GitHub/AppFeed/templates/hotiosRepository/hotio/cloudflare-ddns-ghcr.xml</templatePath>
</Container>
