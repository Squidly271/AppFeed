<?xml version="1.0" encoding="UTF-8"?>
<Container version="2">
  <Name>cassandra</Name>
  <Repository>cassandra:latest</Repository>
  <Registry>https://hub.docker.com/_/cassandra/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/_/cassandra/</Support>
  <Overview>Apache Cassandra is an open-source distributed storage system.</Overview>
  <WebUI></WebUI>
  <TemplateURL>https://raw.githubusercontent.com/dalekseevs/Unraid-Docker-Templates/master/cassandra.xml</TemplateURL>
  <Icon>https://upload.wikimedia.org/wikipedia/commons/thumb/5/5e/Cassandra_logo.svg/279px-Cassandra_logo.svg.png</Icon>
  <ExtraParams>--cap-add sys_nice</ExtraParams>
  <PostArgs></PostArgs>
  <CPUset></CPUset>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9042</HostPort>
        <ContainerPort>9042</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/cassandra</HostDir>
      <ContainerDir>/var/lib/cassandra</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment></Environment>
  <Config Name="Database storage location" Target="/var/lib/cassandra" Default="" Mode="rw" Description="Container Path: /var/lib/cassandra" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/cassandra</Config>
  <Config Name="Port" Target="9042" Default="" Mode="tcp" Description="Container Port: 9042" Type="Port" Display="always" Required="false" Mask="false">9042</Config>
  <Date>1608312154</Date>
  <Description>Apache Cassandra is an open-source distributed storage system.</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/MrChunkysRepository/cassandra.xml</templatePath>
</Container>
