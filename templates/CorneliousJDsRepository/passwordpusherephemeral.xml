  </Networking>
  <Data></Data>
  <Environment></Environment>
  <Config Name="Web Interface" Target="5000" Default="5000" Mode="tcp" Description="Container Port: 5000" Type="Port" Display="always" Required="true" Mask="false">5000</Config>
