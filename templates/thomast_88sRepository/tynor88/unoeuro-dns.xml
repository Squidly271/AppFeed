<?xml version="1.0" encoding="UTF-8"?>
<Container>
  <Name>UnoEuroDNS</Name>
  <Overview>Keep your DNS records for your own domains updated with this UnoEuro DDNS script.</Overview>
  <Support>https://lime-technology.com/forum/index.php?topic=48772.0</Support>
  <Registry>https://hub.docker.com/r/tynor88/unoeuro-dns</Registry>
  <GitHub>https://github.com/tynor88/docker-unoeuro-dns</GitHub>
  <Repository>tynor88/unoeuro-dns</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort></HostPort>
        <ContainerPort></ContainerPort>
        <Protocol></Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>DOMAIN</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>HOSTNAME</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>APIKEY</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>PUSHBULLET_ACCESS_TOKEN</Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Icon>https://raw.githubusercontent.com/tynor88/docker-templates/master/images/unoeuro_small.png</Icon>
  <Date>1475812800</Date>
  <TemplateURL>https://raw.githubusercontent.com/tynor88/docker-templates/master/tynor88/unoeuro-dns.xml</TemplateURL>
  <Category>Network:DNS</Category>
  <Description>Keep your DNS records for your own domains updated with this UnoEuro DDNS script.</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/thomast_88sRepository/tynor88/unoeuro-dns.xml</templatePath>
</Container>
