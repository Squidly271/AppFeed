<?xml version="1.0" encoding="UTF-8"?>
<Container version="2">
  <Name>webtop</Name>
  <Repository>linuxserver/webtop</Repository>
  <Branch>
    <Tag>latest</Tag>
    <Tag>ubuntu-xfce</Tag>
    <Tag>alpine-kde</Tag>
    <Tag>alpine-mate</Tag>
    <Tag>ubuntu-mate</Tag>
    <Tag>alpine-i3</Tag>
    <Tag>alpine-openbox</Tag>
    <Tag>ubuntu-openbox</Tag>
    <Tag>alpine-icewm</Tag>
    <Tag>ubuntu-icewm</Tag>
    <TagDescription>XFCE Alpine</TagDescription>
    <TagDescription>XFCE Ubuntu</TagDescription>
    <TagDescription>KDE Alpine</TagDescription>
    <TagDescription>MATE Alpine</TagDescription>
    <TagDescription>MATE Ubuntu</TagDescription>
    <TagDescription>i3 Alpine</TagDescription>
    <TagDescription>Openbox Alpine</TagDescription>
    <TagDescription>Openbox Ubuntu</TagDescription>
    <TagDescription>IceWM Alpine</TagDescription>
    <TagDescription>IceWM Ubuntu</TagDescription>
  </Branch>
  <Registry>https://hub.docker.com/r/linuxserver/webtop/</Registry>
  <DonateText>Donations</DonateText>
  <DonateLink>https://www.linuxserver.io/donate</DonateLink>
  <DonateImg>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/donate.png</DonateImg>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/linuxserver/docker-webtop/issues/new/choose</Support>
  <Project>https://github.com/linuxserver/docker-webtop</Project>
  <Overview>Webtop - Alpine and Ubuntu based containers containing full desktop environments in officially supported flavors accessible via any modern web browser.
    [br]By default the user/pass is abc/abc</Overview>
  <WebUI>http://[IP]:[PORT:3000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/webtop.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/webtop-logo.png</Icon>
  <ExtraParams>--shm-size=1gb</ExtraParams>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="WebUI" Target="3000" Default="3000" Mode="tcp" Description="Container Port: 3000" Type="Port" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Appdata" Target="/config" Default="" Mode="rw" Description="Appdata" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Description>Webtop - Alpine and Ubuntu based containers containing full desktop environments in officially supported flavors accessible via any modern web browser.
    By default the user/pass is abc/abc</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/linuxserversRepository/linuxserver.io/webtop.xml</templatePath>
</Container>
