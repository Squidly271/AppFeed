<?xml version="1.0" encoding="UTF-8"?>
<Container version="2">
  <Name>firefly-iii-csv-importer</Name>
  <Repository>fireflyiii/csv-importer</Repository>
  <Registry>https://hub.docker.com/r/fireflyiii/csv-importer/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/96221-support-funnypocketbook-firefly-iii</Support>
  <Project>https://github.com/firefly-iii/csv-importer</Project>
  <Overview>A tool to import CSV files into Firefly III</Overview>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/FunnyPocketBook/docker-templates/master/firefly-iii-csv-importer.xml</TemplateURL>
  <Icon>https://camo.githubusercontent.com/794353350b564a5d0c0244cd6d82586c9ffd22f4/68747470733a2f2f7777772e66697265666c792d6969692e6f72672f7374617469632f696d672f6c6f676f2d736d616c6c2d6e65772e706e67</Icon>
  <PostArgs></PostArgs>
  <CPUset></CPUset>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data></Data>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>FIREFLY_III_ACCESS_TOKEN</Name>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Value>http://192.168.0.10:8080</Value>
      <Name>FIREFLY_III_URI</Name>
      <Mode></Mode>
    </Variable>
  </Environment>
  <Config Name="FIREFLY_III_ACCESS_TOKEN" Target="FIREFLY_III_ACCESS_TOKEN" Default="" Mode="" Description="Container Variable: FIREFLY_III_ACCESS_TOKEN - Personal Access Token" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="FIREFLY_III_URI" Target="FIREFLY_III_URI" Default="" Mode="" Description="Container Variable: FIREFLY_III_URI - URI of Firefly-III instance" Type="Variable" Display="always" Required="true" Mask="false">http://192.168.0.10:8080</Config>
  <Config Name="Configuration Path" Target="/configurations" Default="" Mode="rw" Description="Container Path: /configurations - Path to pre-made configuration file" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/firefly-iii-csv-importer/configurations</Config>
  <Config Name="JSON_CONFIGURATION_DIR" Target="JSON_CONFIGURATION_DIR" Default="" Mode="" Description="Container Variable: JSON_CONFIGURATION_DIR - Container path of configuration storage&#13;&#10;" Type="Variable" Display="always" Required="false" Mask="false">/configurations</Config>
  <Config Name="Port" Target="8080" Default="" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="false" Mask="false">8081</Config>
  <Date>1598665841</Date>
  <Category>Productivity:</Category>
  <Description>A tool to import CSV files into Firefly III</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/FunnyPocketBooksRepository/firefly-iii-csv-importer.xml</templatePath>
</Container>
