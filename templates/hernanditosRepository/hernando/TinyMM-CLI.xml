<?xml version="1.0" encoding="UTF-8"?>
<Container>
  <Name>TinyMM-CLI</Name>
  <Overview>
   Tiny Media Manager  CLI with Cron. 
  </Overview>
  <Registry>https://registry.hub.docker.com/r/coolasice1999/tmm-cli-cronjob/</Registry>
  <Repository>coolasice1999/tmm-cli-cronjob:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5800</HostPort>
        <ContainerPort>5800</ContainerPort>
        <Protocol></Protocol>
      </Port>
      <Port>
        <HostPort>5801</HostPort>
        <ContainerPort>5801</ContainerPort>
        <Protocol></Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>USER_ID</Name>
      <Value>0</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>GUID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Media/</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/tmm/config/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5800]</WebUI>
  <Icon>https://i.imgur.com/M5Io6sF.png</Icon>
  <Support>http://lime-technology.com/forum/index.php?topic=39623.0</Support>
  <TemplateURL>https://raw.githubusercontent.com/hernandito/docker-templates/master/hernando/TinyMM-CLI.xml</TemplateURL>
  <Category>Other:Uncategorized</Category>
  <Description>Tiny Media Manager  CLI with Cron.</Description>
  <templatePath>/tmp/GitHub/AppFeed/templates/hernanditosRepository/hernando/TinyMM-CLI.xml</templatePath>
</Container>
