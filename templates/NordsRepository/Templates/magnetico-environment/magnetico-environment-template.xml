  <Name>magnetico-environment</Name>
  <Repository>dyonr/magnetico-environment</Repository>
  <Registry>https://hub.docker.com/r/dyonr/magnetico-environment</Registry>
  <Network>bridge</Network>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
